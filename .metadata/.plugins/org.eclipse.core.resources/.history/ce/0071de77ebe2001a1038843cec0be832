#include <string>
#include <cstdio>
#include "Planet.hpp"
using namespace std;

Planet::Planet(double positionx,double positiony,double kg,double velocityx,double velocityy, string name){
	posx=positionx;
	posy=positiony;
	mass= kg;
	velx=velocityx;
	vely=velocityy;
	this->name=name;

}

Planet::Planet(){
	posx=0;
	posy=0;
	mass= 0;
	velx=0;
	vely=0;
	name="unknown";
}

string Planet::getName(){
	return name;
}

void Planet::setName(string name){
	this->name=name;
}


