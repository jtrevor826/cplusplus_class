#include <string>
using namespace std;
#ifndef PLANET_HPP_
#define PLANET_HPP_

class Planet{
	private:
	double mass;
	double posx;
	double posy;
	double velx;
	double vely;
	string name;

	public:
	Planet();
	Planet(double positionx,double positiony,double kg,double velocityx,double velocityy, string name);
	string getName();
	void setName(string name);
};

#endif /* PLANET_HPP_ */
